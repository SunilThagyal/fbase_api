{
  "root": true,
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "quotes": ["error", "double"],
    "require-jsdoc": "off",
    "max-len": ["error", { "code": 120 }],
    "object-curly-spacing": ["off"],
    "no-unused-vars": ["off"],
    "comma-dangle": ["off"],
    "eol-last": ["off"]
  }
}
